+def ms_converter(millis):
+    millis = int(millis)
+    seconds = (millis / 1000) % 60
+    seconds = int(seconds)
+    if str(seconds) == '0':
+        seconds = '00'
+    if len(str(seconds)) == 1:
+        seconds = '0' + str(seconds)
+    minutes = (millis / (1000 * 60)) % 60
+    minutes = int(minutes)
+    return str(minutes) + ":" + str(seconds)
